{
  "name": "apply-flow-service",
  "version": "1.0.0",
  "description": "Multi-tenant gateway service for orchestrating lending product behavior",
  "main": "dist/index.js",
  "author": "Earnest Inc.",
  "license": "UNLICENSED",
  "scripts": {
    "build": "rm -rf ./dist && npx chassis-tsc",
    "lint": "npx chassis-lint && prettier --check '{src,test}/**/*.{js,ts}'",
    "test": "npx chassis-test",
    "lint:prettify": "prettier --write '{src,test}/**/*.{js,ts}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetearnest/apply-flow-service.git"
  },
  "logging": {
    "level": "http"
  },
  "bugs": {
    "url": "https://github.com/meetearnest/apply-flow-service/issues"
  },
  "homepage": "https://github.com/meetearnest/apply-flow-service#readme",
  "dependencies": {
    "@earnest-labs/microservice-chassis": "2.1.x",
    "@earnest-labs/ts-sensitivestring": "^0.1.26",
    "@earnest/application-service-client": "^1.1.3",
    "@earnest/http": "^2.0.0",
    "@earnest/microservice-chassis-secret-loader": "^0.1.19",
    "@earnest/microservice-chassis-traceid-express-middleware": "^1.0.2",
    "@earnest/state-machine": "^2.2.0",
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.6.0",
    "cookie-parser": "^1.4.6",
    "gql-query-builder": "^3.8.0",
    "handlebars-a-la-json": "^1.5.8",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.1",
    "objection": "^3.0.4",
    "pg": "^8.11.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3"
  },
  "build": "localdev",
  "_moduleAliases": {
    "@src": "src",
    "@test": "test",
    "@types": "src/types"
  },
  "c8": {
    "reporter": [
      "html",
      "text"
    ],
    "lines": 80,
    "branches": 75,
    "statements": 80
  },
  "type": "module"
}
