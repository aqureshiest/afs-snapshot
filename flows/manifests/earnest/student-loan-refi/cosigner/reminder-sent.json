{
  "*": "stage",
  "stage": {
    "*": "stage.view",
    "stageKey": {
      "@": "reminder-sent"
    },
    "stageLabel": {
      "@": "Reminder Sent"
    },
    "stageLayout": {
      "*": "stage/layout",
      "header": [
        {
          "*": "stage/layout/header.logo",
          "action": "actions/navigate.homebase"
        }
      ]
    },
    "bottomNavigation": {
      "*": "stage.bottom-navigation",
      "buttons": [{
        "*": "actions.with-copy",
        "copy": {
          "@": "Back to dashboard"
        },
        "action": {
          "*": "actions/navigate",
          "goTo": "earnest-student-loan-refi/urls.degree"
        }
      }]
    },
    "stageContent": [
      {
        "*": "flexible-content/items/header",
        "centered": {
          "@": "true"
        },
        "asset": {
          "*": "asset/local",
          "icon": "asset/local/illustrations.success",
          "centered": {
            "@": "true"
          }
        },
        "copy": {
          "@": "Your reminder has been sent!"
        },
        "subCopy": {
          "@": "We sent a reminder to {cosigner.input@email.com} \n\n Youâ€™ll be notified once your cosigner has completed their portion of the application."
        },
        "containerProps": {
          "@": { "flexGrow": 1, "alignContent": "center" }
        }
      }
    ]
  }
}