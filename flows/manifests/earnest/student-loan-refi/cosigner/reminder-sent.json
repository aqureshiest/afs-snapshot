{
  "*": "stage",
  "stage": {
    "*": "stage.view",
    "stageKey": {
      "@": "reminder-sent"
    },
    "stageLabel": {
      "@": "Reminder Sent"
    },
    "analytics": "actions/analytics/full-refi/cosigner.reminder-sent-page-event",
    "stageLayout": {
      "*": "stage/layout",
      "header": [
        {
          "*": "stage/layout/header.logo",
          "action": "actions/navigate.homebase"
        },
        {
          "*": "stage/layout/header.sidebar",
          "items": [
            "stage/layout/header/sidebar-items.rates",
            "stage/layout/header/sidebar-items.support"
          ]
        }
      ]
    },
    "bottomNavigation": {
      "*": "stage.bottom-navigation",
      "buttons": [{
        "*": "actions.with-copy",
        "copy": {
          "@": "Back to dashboard"
        },
        "action": {
          "*": "actions/navigate.homebase"
        }
      }]
    },
    "stageContent": [
      {
        "*": "flexible-content/items/header",
        "centered": {
          "@": "true"
        },
        "asset": {
          "*": "asset/local",
          "icon": "asset/local/illustrations.success",
          "centered": {
            "@": "true"
          }
        },
        "copy": {
          "@": "Your reminder has been sent!"
        },
        "subCopy": "earnest-student-loan-refi/pages/reminder-sent.email-copy",
        "containerProps": {
          "@": { "flexGrow": 1, "alignContent": "center" }
        }
      }
    ]
  }
}