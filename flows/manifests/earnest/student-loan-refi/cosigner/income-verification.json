[
  {
    "methods": ["get"],
    "parameters": [{
      "key": "id",
      "format": "uuid"
    }],
    "inputs": {
      "*": "inputs",
      "application": {
        "*": "application/single.flattened",
        "application": "application-by-id-param"
      },
      "applications": [],
      "applicationState": "application-state",
      "userState": "user-state",
      "env": "inputs/env.plaid",
      "auth": {
        "*": "auth-artifacts",
        "strategies": [
          {
            "*": "inputs/auth.neas-session",
            "response": "neas.verify-token",
            "authorization": [
              "inputs/auth.neas-application-auth",
              "inputs/auth.neas-user-auth"
            ]
          }
        ]
      }
    },
    "outputs": {
      "*": "income-verification",
      "stage": "plaid-income-verification.full-refi",
      "analytics": "actions/analytics/full-refi/cosigner.income-verification-page-event",
      "plaid-link-token-method": "plaid.generate-link-token",
      "section": "refi-primary-only-section",
      "back-button": "actions/back-buttons/full-refi/cosigner.income",
      "pageLayout": {
        "*": "stage/layout",
        "header": [
          {
            "*": "stage/layout/header.logo",
            "action": "actions/navigate.homebase"
          },
          {
            "*": "stage/layout/header.sidebar",
            "items": [
              "stage/layout/header/sidebar-items.rates",
              "stage/layout/header/sidebar-items.support"
            ]
          }
        ]
      },
      "progress": [
        {
          "*": "stage.progress-item",
          "label": {
              "@": "Apply"
          },
          "value": {
            "@": "90"
          }
        }
      ],
      "onError": ["onError.unauthorized"],
      "redirect_homebase": "actions/homebase.default"
    }
  }
]
