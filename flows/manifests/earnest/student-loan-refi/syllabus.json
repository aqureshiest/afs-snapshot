{
  "*": "stage",
  "stage": {
    "*": "stage.view",
    "stageKey": {
      "@": "get-rates"
    },
    "stageLabel": {
      "@": "Current Address"
    },
    "stageLayout": {
      "*": "stage/layout",
      "header": [
        {
          "*": "stage/layout/header.logo",
          "action": "actions/navigate.homebase"
        }
      ],
      "banner": {
        "*": "stage/layout/banner/templates.cosigner-finished-rates",
        "primaryAction": {
          "*": "actions.with-copy",
          "text": {
            "@": "View rates"
          },
          "action": {
            "*": "actions/modal.manifest",
            "manifest": "earnest-student-loan-refi/urls/content.rates-modal"
          }
        }
      }
    },
    "analytics": "analytics/full-refi.syllabus",
    "bottomNavigation": {
      "*": "stage.bottom-navigation",
      "buttons": [{
        "*": "actions.with-copy",
        "copy": {
          "@": "Send reminder to cosigner"
        },
        "action": {
          "*": "actions/navigate",
          "goTo": "earnest-student-loan-refi/urls.reminder-sent"
        }
      }]
    },
    "stageContent": [
      {
        "*": "flexible-content/items/header",
        "copy": {
          "@": "Your application at a glance"
        },
        "subCopy": {
          "@": "Youâ€™re almost done with submitting your full application - send your cosigner an reminder to finish their portion."
        }
      },
      {
        "*": "flexible-content/items/progress-card",
        "label": {
          "@": "Primary's info"
        },
        "status": {
          "*": "asset/local",
          "icon": "asset/local/icons.green-check"
        },
        "progress": {
          "@": "100"
        }
      },
      {
        "*": "flexible-content/items/progress-card",
        "label": {
          "@": "Cosigner's info"
        },
        "status": { "@": "in progress"},
        "progress": {
          "@": "50"
        },
        "containerProps": {
          "@": {"marginTop": "24px"}
        }
      }
    ]
  }
}