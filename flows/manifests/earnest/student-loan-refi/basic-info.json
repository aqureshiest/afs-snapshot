{
    "*": "full-refi",
    "stage": "student-info-stage.full-refi",
    "pageLayout": {
      "*": "stage/layout",
      "header": [
        {
          "*": "stage/layout/header.logo",
          "action": "actions/navigate.homebase"
        },
        {
          "*": "stage/layout/header.sidebar",
          "items": ["stage/layout/header/sidebar-items.rates", "stage/layout/header/sidebar-items.support"]
        }
      ]
    },
    "steps": [
      "student-info-form/basic.full-refi"
    ],
    "student-info-basic-rows": [
      "info/student-info.refi",
      "questions/primary/name.default",
      "questions/primary/dateOfBirth.default",
      "questions/primary/phone.default",
      "questions/primary/email.full-refi",
      "info/student-disclaimer.full-refi"
    ],
    "progress": [
      {
        "*": "stage.progress-item",
        "label": {
          "@": "Get Rates"
        },
        "value": {
          "@": "15"
        }
      }
    ],
    "back-button": "actions/back-buttons/full-refi.basic-info",
    "save_basic_info_details_action": "actions/save-details/full-refi.basic-info",
    "primary_dob_input_status": "questions/primary/dateOfBirth.status",
    "primary_phone_input_status": "questions/primary/phone.status",
    "primary_name_input_status": "questions/primary/name.status",
    "redirect_homebase": "actions/homebase.default",
    "onError": []
  }