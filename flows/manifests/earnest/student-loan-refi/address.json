{
  "*": "stage",
  "stage": {
    "*": "stage.form",
    "stageKey": {
      "@": "get-rates"
    },
    "stageLabel": {
      "@": "Current Address"
    },
    "stageValidation": {
      "*": "earnest-student-loan-refi/pages/address.validation",
      "current-address-validation": {
        "*": "questions/primary/address/validation"
      }
    },
    "stageLayout": {
      "*": "stage/layout",
      "header": [
        {
          "*": "stage/layout/header.logo",
          "action": "actions/navigate.homebase"
        }
      ]
    },
    "stageSteps": [
      {
        "*": "stage.form-step",
        "stepKey": {
          "@": "address-step"
        },
        "stepLabel": {
          "@": "Current Address"
        },
        "stepProgress": [
          {
            "*": "stage.progress-item",
            "label": {
              "@": "Get Rates"
            },
            "value": {
              "@": "20"
            }
          }
        ],
        "stepBackAction": {
          "*": "actions/navigate",
          "goTo": "earnest-student-loan-refi/urls.degree"
        },
        "stepSaveAction": {
          "*": "actions/request",
          "key": {
            "@": "save-address"
          },
          "manifest": "earnest-student-loan-refi/urls/save.address"
        },
        "onError": [],
        "stepSaveCopy": {
          "@": "Citizenship"
        },
        "stepRows": [
          {
            "*": "info/header",
            "key": {
              "@": "header"
            },
            "copy": {
              "@": "Address"
            },
            "subCopy": {
              "@": "Enter the permanent address, typically shown on your ID or driverâ€™s license. "
            }
          },
          {
            "*": "questions/primary/address",
            "expanded": {
              "@": "true"
            },
            "baseLabel": {
              "@": "Address"
            }
          }
        ]
      }
    ]
  }
}