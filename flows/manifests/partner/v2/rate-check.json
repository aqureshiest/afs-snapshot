[
  {
    "inputs": {
      "<": "inputs",
      "application": {
        "<": "application/single.v2",
        "application": "application-by-id-param",
        "query": "application/lookup-query.rate-check"
      },
      "env": {
        "<": "inputs/env.combine",
        "groups": [
          "inputs/env.pii",
          "inputs/env.decisioning"
        ]
      },
      "auth": {
        "<": "auth-artifacts",
        "strategies": [
          {
            "<": "inputs/auth.internal-bearer",
            "token": "inputs/auth.bearer-token",
            "authorization": [
              "inputs/auth.partner-internal"
            ]
          }
        ]
      }
    }
  },
  {
    "methods": [
      "post",
      "put",
      "patch"
    ],
    "parameters": [
      {
        "key": "id",
        "format": "uuid"
      }
    ],
    "inputs": {
      "validation": [
        {
          "<": "request-body",
          "schema": "inputs/schema/request/rate-estimate"
        }
      ]
    },
    "outputs": {
      "<": "response.application",
      "application": {
        "<": "application/single.pick-id",
        "application": {
          "<": "application/single.v2",
          "application": {
            "<": "application-from-id",
            "query": "application/lookup-query.rate-check",
            "application": {
              "<": "create.upsert",
              "payload": {
                "<": "partner/payload",
                "ssn": "pii.save-partner-token"
              }
            }
          }
        },
        ">": [
          {
            "<": "save-decision-id",
            "decision": {
              "<": "decisioning/rate-estimate",
              "role": {
                "@": "primary"
              },
              "decisionType": {
                "@": "rate-check"
              },
              "action": {
                "@": "rate-estimate"
              },
              "school": "school.get-by-opeid",
              "basics": "lds/payload/basics",
              "appInfo": "lds/payload/appInfo"
            }
          }
        ]
      }
    }
  },
  {
    "methods": [
      "get"
    ],
    "inputs": {
      "env": {
        "<": "inputs/env.combine",
        "groups": [
          "inputs/env.calculator",
          "inputs/env.decisioning"
        ]
      }
    },
    "parameters": [
      {
        "key": "id",
        "format": "uuid",
        "required": true
      }
    ],
    "outputs": {
      "<": "response/rate-estimate",
      "application": {
        "<": "application/single.v2",
        "application": {
          "<": "application-from-id",
          "query": "application/lookup-query.rate-check"
        }
      },
      "artifacts": "decisioning/rate-estimate.artifact",
      "status": "rates/status",
      "rates": {
        "<": "rates.partner",
        "rates": {
          "<": "calculator/get-min-payment-price",
          "termsToInclude": {
            "@": [
              60,
              84,
              120,
              180,
              240
            ]
          }
        }
      }
    }
  }
]
