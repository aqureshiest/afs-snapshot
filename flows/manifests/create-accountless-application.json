[{
  "methods": ["post"],
  "inputs": {
    "*": "inputs",
    "auth": {
      "*": "auth-artifacts",
      "strategies": [
        {
          "*": "inputs/auth.neas-session",
          "response": {
            "*": "neas.verify-token",
            "token": "header.idtoken"
          },
          "mode": "optional",
          "authorization": []
        }
      ]
    },
    "env": {
      "*": "inputs/env.combine",
      "groups": [
        "inputs/env.ui"
      ]
    }
  },
  "outputs": {
    "*": "redirect.accountless",
    "actions": {
      "*": "actions/result.results",
      "actions": [
        "create.with-applicant",
        "neas.accountless-session",
        "session"
      ]
    }
  }
}]
