{
    "key": "404-page",
    "type": "view",
    "label": "404 Page",
    "definition": {
      "type": "landing",
      "definition": {
        "key": "404-landing",
        "centered": true,
        "header": {
          "centered": true,
          "asset": {
            "type": "LocalAsset",
            "center": true,
            "name": "illustrations.warning"
          },
          "copy": "Page Not Found"
        },
        "primaryAction": {
          "copy": "Go back to Home page",
          "action": {
            "key": "404-resume",
            "type": "navigate",
            "properties": {
              {{#if application.id}}
                "goTo": "/resume/{{application.id}}"
              {{else}}
                "goTo": "{{env.BASE_URL}}",
                "external": true
              {{/if}}
            }
          }
        }
      }
    }
  }
