{{#if request.params.id}}
{
  "type": "view",
  "id": "{{{request.params.id}}}",
  "definition": {
    "type": "view",
    "definition": {
        "key": "income-verification-review",
        "financialAccounts":
          {{#list}}
            {{#each application.details.financialAccounts}}
             {"index": {{this.index}}, "name": "{{this.name}}", "account_last4": "{{this.account_last4}}", "type": "{{this.type}}", "selected": "{{this.selected}}" }
            {{/each}}
          {{/list}}
        
    }
  }
}
{{else}}
{
  "statusCode": 401,
  "message": "Id param is required."
}
{{/if}}

