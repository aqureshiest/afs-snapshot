{{#if request.params.id}}
  {{#obj}}
    {{#multiline~}}
      {
        "id": {{{ json request.params.id }}},
        "required": true,
        "root": true
      }
    {{~/multiline}}
    {{#with (contract 'query') as |query|}}
      { "query": {{{ json query }}} }
    {{/with}}
  {{/obj}}
{{else}} null {{/if}}
