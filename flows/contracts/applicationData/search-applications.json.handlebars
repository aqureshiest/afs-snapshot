{
  "criteria": {{#list}}
    {{#with auth.artifacts}}
      {{#if userId}}
      { "monolithUserID": "{{ userId }}" }
      {{/if}}
    {{/with}}
  {{/list}}
  {{#with request.query}}
    {{#if limit}}
      ,"limit": {{{json limit}}}
    {{/if}}
    {{#if page}}
      ,"page": {{{json page}}}
    {{/if}}
  {{/with}}
}
