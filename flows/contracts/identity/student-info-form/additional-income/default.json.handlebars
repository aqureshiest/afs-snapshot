{{#with application.primary}}
{
    "key": "additionalIncome",
    "label": "Student Income Info",
    "submit": {
        "copy": "Next Step",
        "action": {{{json (contract 'save_primary_details_action')}}}
    },
    "conditional": [
        [
            {
                "field": "employmentStatus.employmentType", {{! field is stepKey.fieldKey}}
                "op": "valueEquals",
                "value": "retired"
            }
        ]
        
    ],
    "rows": {{{json (contract 'student-info-income-retired-additional-income-rows')}}}
    
}
{{/with}}