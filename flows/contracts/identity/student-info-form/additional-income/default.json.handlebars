{{#with application.primary}}
{
    "key": "additionalIncome",
    "label": "income",
    "analytics": {
        "product": "slr",
        "name": "Income Info",
        "title": "Retired Income"
    },
    "submit": {
        "copy": "Next Step",
        "action": {{{json (contract 'save_primary_retired_income_details_action')}}}
    },
    "conditional": [
        [
            {
                "field": "employmentStatus.employmentType", 
                "op": "valueEquals",
                "value": "retired"
            }
        ]
    ],
    "progress": 90,
    "rows": {{{json (contract 'student-info-income-retired-additional-income-rows')}}}
    
}
{{/with}}