{{#with application.primary}}
{
    "key": "sourcesOfIncome",
    "label": "Student Source Income Info",
    "submit": {
        "copy": "Next Step",
        "action": {{{json (contract 'save_primary_details_action')}}}
    },
    "rows": {{{json (contract 'student-info-income-retired-rows')}}},
    "conditional": [
        [
            {
                "field": "employmentStatus.employmentType", {{! field is stepKey.fieldKey}}
                "op": "valueEquals",
                "value": "retired"
            }
        ]
        
    ]
    
}
{{/with}}