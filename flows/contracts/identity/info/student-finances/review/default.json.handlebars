{{#with application.primary}}
{
    "type": "info",
    "definition": {
        "key": "incomeInfoReview",
        "type": "infoReview",
        "label": "Income Verification",
        "items": {{#list}}
            {{#each details.financialAccounts}}
             {{#if (eq (boolean this.selected) true) }} {"id": "{{number @index}}","label": "{{this.name}}", "value": "{{this.name}} - {{this.account_last4}}" }{{else}}{}{{/if}}
            {{/each}}
          {{/list}},
        "editAction": {
            "key": "",
            "type": "navigate",
            "properties": {
                 "analytics": {
                    "name": "Action Clicked",
                    "action": "edit income verification",
                    "title": "Review",
                    "product": "{{{mapProduct ../application.reference.product}}}",
                    "loan_type": "{{{mapLoanType ../application.tag.applicants}}}"
                },
                "goTo": "earnest/student-refi/income-verification/{{id}}"
            }
        }
    }
}
{{/with}}