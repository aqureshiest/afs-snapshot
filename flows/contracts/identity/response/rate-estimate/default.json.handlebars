{{! Status and rates for partner rate-check API }}
{{#obj}}
  { "status": {{{ json (contract 'status') }}} }
  {{#with (contract 'application') as |application|}}
    { "applicationUrl": {{{ json application.href }}} }
  {{else}}
    { "applicationUrl": null }
  {{/with}}
  { "rates": {{{ json (contract 'rates') }}} }
{{/obj}}
