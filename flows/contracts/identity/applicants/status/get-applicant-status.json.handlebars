{{#if (contract 'actions' 0) }}
  {{#with (contract 'updatedApplication') as |updatedApplication| }}
    {{#if updatedApplication}}
      {{#list}}
        {{#each updatedApplication.applicants as |applicant|}}
          "{{applicant.status.name}}"
        {{/each}}
      {{/list}}
    {{else}}
      null
    {{/if}}
  {{else}}
    null
  {{/with}}
{{else}}
  null
{{/if}}