{{#if request.params.id }}
  {{#if (hasValues applicationState) }}
    {{#with applicationState }}
      {{#if step}}
        "{{manifest}}?step={{step}}"
      {{else}}
        "{{manifest}}"
      {{/if}}
    {{/with}}
  {{else}}
  "{{application.brand}}/{{mapProductForRedirect application.product}}{{#if (eq application.cosigner.id (lookup (getApplicantByEmail auth.artifacts.email application) "id"))}}/cosigner{{/if}}/{{(lookup (getApplicantByEmail auth.artifacts.email application) "id")}}"
{{else}}
  null
{{/if}}
