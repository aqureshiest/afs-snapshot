{
  "componentProps": {
    {{#if (contract "action")}}
      "action": {{{json (contract "action")}}},
    {{/if}}
    "status": {{{json (maybe (contract "status") "in progress")}}},
    "progress": {{{json (number (maybe (contract "progress") 0))}}},
    {{#if (contract "componentProps")}}
      {{#spread trailingComma=true}}
        {{{json (contract "componentProps")}}}
      {{/spread}}
    {{/if}}
    "label": {{{json (maybe (contract "label") "Add a label")}}}
  },
  {{#if (contract "containerProps")}}
    "containerProps": {{{json (contract "containerProps")}}},
  {{/if}}
  "key": {{{keyString "progress-card"}}},
  "type": "progressCard"

}