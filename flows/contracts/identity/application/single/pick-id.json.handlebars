{{#with (contract 'application') as |application|}}
  {{#obj}}
    { "id": {{{json application.id }}} }
    {{#if application.primary}}
      { "primary": {{{ json application.primary.id }}} }
    {{/if}}
    {{#if application.cosigner}}
      { "cosigner": {{{ json application.cosigner.id }}} }
    {{/if}}
  {{/obj}}
{{else}}
  null
{{/with}}
