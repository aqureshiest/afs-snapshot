{
    "key": "save-details",
    "type": "request",
    "properties": {
        "analytics": {
            "name": "Application Section Submitted",
            "product": "slr",
            "section": "primary info",
            "loan_type": "{{{mapLoanType application.tag.applicants}}}"
        },
        "manifest": "save-details/{{application.primary.id}}",
        "method": "POST",
        "onError": [{
            "codes": ["unathorized"],
            "action": {
                "key": "unauthorized-error",
                "type": "navigate",
                "properties": {
                    "goTo": "{{env.BASE_URL}}/_/auth/login?targetUrl=/_/apply/resume/{{request.params.id}}"
                }
            }
        }]
    }
}