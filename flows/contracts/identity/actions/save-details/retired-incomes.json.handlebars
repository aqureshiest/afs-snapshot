{
    "key": "save-details",
    "type": "request",
    "properties": {
        "manifest": "save-details/retired-incomes/{{application.primary.id}}",
        "method": "POST",
        "onError": [{
            "codes": ["unathorized"],
            "action": {
                "key": "unauthorized-error",
                "type": "navigate",
                "properties": {
                    "goTo": "{{env.BASE_URL}}/_/auth/login?targetUrl=/_/apply/resume/{{request.params.id}}"
                }
            }
        }]
    }
}