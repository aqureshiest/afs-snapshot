{{!
  Generic action result descriptor for client-method contracts
}}
{{#with (contract 'action') as |result|}}
  {
    "action": {{{ json result.action }}},
    "success": {{{ json (
      and
      (gte result.response.statusCode 200)
      (lt result.response.statusCode 400)
    )}}}
  }
{{else}}
{
  "action": null,
  "success": false
}
{{/with}}
