{{#with application.primary}}
{{!#if (or (eq details.income.0.type "self_employed") (eq deatils.income.0.type "future"))}}
{
  "type": "input",
  "definition": {
      "key": "startDate",
      "type": "inputDate",
      "value":  "{{{details.income.0.start}}}",
      "status": {{{json (contract 'primary_income_start_date_input_status')}}},
      "label": {
        "copy": "Start date"
      },
      "error": "{{{ ajv 'errors' 'questions/primary/employment/start-date'  details.income.0.start }}}",
      "condtional": [
        [
            {
                "field": "employment.employmentType", {{! field is stepKey.fieldKey}}
                "op": "valueEquals",
                "value": "self_employed"
            }
        ],
        [
            {
                "field": "employment.employmentType", {{! field is stepKey.fieldKey}}
                "op": "valueEquals",
                "value": "future"
            }
        ]
      ]
  }
}
{{!/if}}
{{/with}}