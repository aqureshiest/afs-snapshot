{{#applicantById}}
{
    "type": "input",
    "definition": {
        "key": {{{json (contract 'key')}}},
        "conditional": {{{maybe (json (contract 'conditional')) []}}},
        {{#if (contract "value")}}
          "value": {{{json (contract 'value')}}},
        {{else}}
          "value": {
            "month": "",
            "day": "",
            "year": ""
          },
        {{/if}}
        "label": {
          {{#if ../features.cis_person}}
            "copy": "CIS PERSON FLAG IS ON YO"
          {{else}}
            "copy": {{{json (contract 'label')}}}
          {{/if}}
        },
        "omitDay": {{{json (contract 'omitDay')}}},
        {{#if (contract "deps")}}"deps":{{{json (contract 'deps')}}},{{/if}}
        {{#if (contract 'error')}}
          "error": {{{json (contract 'error')}}},
        {{/if}}
        "type": "inputDate"
    }
}
{{/applicantById}}