{
  "minLength": 1,
  "type": "string",
  "pattern": "^[a-zA-Z0-9\\s.'-]*$",
  "matches": {
    "shouldMatch": false,
    "useParentData": true,
    "matchAgainstPath": "editPersonalInfo.currentAddress"
  },
  "errorMessage": {
    "minLength": "Street address is required.",
    "pattern": "Street contains invalid characters.",
    "matches": "Your previous address cannot match your current address."
  }
}