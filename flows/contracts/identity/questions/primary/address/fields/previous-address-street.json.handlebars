{{#with application.primary}}
  {
    "minLength": 1,
    "type": "string",
    "pattern": "^[a-zA-Z0-9\\s.'-]*$",
    "matches": {
      "shouldMatch": false,
      "useParentData": true,
      "matchAgainstPath": "currentAddress.currentAddress"
    },
    "errorMessage": {
      "minLength": "Street is required",
      "pattern": "Invalid street",
      "matches": "Your previous address cannot match your current address"
    }
  }
{{/with}}