{{#with application.primary}}
  {
    "minLength": 1,
    "type": "string",
    "pattern": "^[a-zA-Z0-9\\s.'-]*$",
    "matches": {
      "shouldMatch": false,
      "useParentData": true,
      "matchAgainst": {
        "street": "{{{ details.location.0.street1}}}",
        "apt": "{{{ details.location.0.street2}}}",
        "city": "{{{ details.location.0.city}}}",
        "state": "{{{ details.location.0.state}}}",
        "zip": "{{{ details.location.0.zip}}}"
      }
    },
    "errorMessage": {
      "minLength": "Street is required",
      "pattern": "Invalid street",
      "matches": "Your previous address cannot match your current address"
    }
  }
{{/with}}