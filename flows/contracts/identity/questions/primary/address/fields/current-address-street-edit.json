{
  "minLength": 1,
  "type": "string",
  "pattern": "^[a-zA-Z0-9\\s.'-]*$",
  "matches": {
    "shouldMatch": false,
    "useParentData": true,
    "matchAgainstPath": "editPersonalInfo.previousAddress"
  },
  "errorMessage": {
    "minLength": "Street is required",
    "pattern": "Invalid street",
    "matches": "Your current address cannot match your previous address"
  }
}