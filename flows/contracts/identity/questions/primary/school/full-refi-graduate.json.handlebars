{{#with application.primary}}
{
  "type": "input",
  "definition": {
      "key": "school",
      "type": "inputSchool",
      "status": {{{json (contract 'primary_school_input_status')}}},
      {{! TODO: request school name based on opeid from accredited-school-service}}
      "value": "{{{details.education.0.opeid}}}",
      "label": {
          "copy": "Graduate school"
      },
      "conditional": [
        [{
          "field": "EducationInfo.degree-type",
          "op": "valueEquals",
          "value": "graduate"
        }]
      ]
  }
}
{{/with}}