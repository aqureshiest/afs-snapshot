{{#with application.primary}}
{
    "type": "input",
    "definition": {
        "key": "email",
        "type": "inputEmail",
        "placeholder": "Email",
        "status": {{{json (contract 'primary_email_input_status')}}},
        "value": "{{{ details.email }}}",
        "label": {
            "copy": "Email"
        },
        "error": "{{{ajv 'errors' 'questions/cosigner/email' details.email}}}",
        "hint": ""
    }
}
{{/with}}