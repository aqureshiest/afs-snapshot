{
  "header": [
    {
      "logo": true,
      "action": {{{ json (contract 'redirect_homebase') }}}
    },
    {
      "icon": "HamburgerMenuIcon",
      "action": {
        "key": "sidebar",
        "type": "sidebar",
        "properties": {
          "type": "navigation",
          "properties": {
            "dark": true,
            "items": [
              {
                "copy": "Sign Out",
                "action": {
                  "type": "logout",
                  "properties": {
                    {{!-- TODO: once env variables are passed, dynamically set goTo--}}
                    "goTo": "{{env.BASE_URL}}",
                    "external": true
                  }
                }
              },
              {
                "copy": "View Rates",
                "action": {
                  "type": "navigate",
                  "properties": {
                    {{!-- Need to figure out how to populate the appId --}}
                    "goTo": "{{env.LEGACY_RATES_URL}}?applicationId={{this.application.primary.reference.monolithApplicationID}}",
                    "external": true
                  }
                }
              }
            ]
          }
        }
      }
    }
  ]
}