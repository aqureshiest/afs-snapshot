{
  "header": [
    {
      "logo": true,
      {{#if auth?.session?.userId}}
        "action": {{{ json (contract 'redirect_homebase') }}}
      {{else}}
        "action": {{{ json (contract 'redirect_product_landing') }}}
      {{/if}}
    },
    {
      "icon": "HamburgerMenuIcon",
      "action": {
        "key": "sidebar",
        "type": "sidebar",
        "properties": {
          "type": "navigation",
          "properties": {
            "dark": true,
            "items": [
              {
                "copy": "Sign Out",
                "action": {
                  "type": "logout",
                  "properties": {
                    {{!-- TODO: once env variables are passed, dynamically set goTo--}}
                    "goTo": "{{env.BASE_URL}}",
                    "external": true
                  }
                }
              }
            ]
          }
        }
      }
    }
  ]
}