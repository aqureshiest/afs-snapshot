{{#with application}}
  {{#if (eq root null)}}
    {{#if (gt applicants.length 1)}}
      {{#each applicants}}
      
        {{#if (ajv 'validate' 'applicant-id-from-root/primary' this)}}
    
          "{{this.id}}"
          {{break}}
        {{/if}}
      {{/each}}
    {{else if (eq applicants.length 1)}}
    
      "{{applicants.0.id}}"
    {{/if}}
  {{/if}}
{{/with}}
