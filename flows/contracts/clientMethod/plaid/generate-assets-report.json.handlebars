{
  "action": "generate-assets-report",
  "client": "plaid",
  "method": "post",
  "uri": "/asset_report/create",
  "body": {
    "access_tokens": {{{json (contract 'actions' (sum @index -1))}}},
    "days_requested": {{{env.PLAID_ASSETS_REPORT_DAYS_REQUESTED}}},
    "client_id": "{{{env.PLAID_CLIENT_ID}}}",
    "secret": "{{{env.PLAID_SECRET}}}"
  },
  "headers": {
    "Content-Type": "application/json",
    "Plaid-Version": "2020-09-14"
  }
}