{
  "action": "attribution_application_submitted",
  "client": "partnerClient",
  "method": "post",
  "uri": "/api/attribution/application-submit",
  "body": {
    "application_id": "{{application.id}}",
    "user_id": {{{ json (coalesce application.primary.reference.userID application.primary.reference.userIdBeforeVerifyingThroughEmailId) }}},
    "source": "apply-flow-service",
    "misc": {
      "email": "{{application.primary.details.email}}"
    }
  }
}