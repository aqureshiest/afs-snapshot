{
  "action": "send-verification-email",
  "client": "NeasClient",
  "method": "post",
  "uri": "auth/identity/instant-access-code/send",
  "body": {
    "applicationId": {{{json application.id}}},
    "emailId": {{{json (lookup (lookup (getApplicant request.params.id application) "details") "email")}}}
  }
}
