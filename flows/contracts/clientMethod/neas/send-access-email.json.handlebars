{{#with (contract 'session' (sum @index -1)) as |session|}}
  {
    "action": "send-access-email",
    "client": "NeasClient",
    "method": "post",
    "uri": "auth/identity/access-code/send",
    "body": {
      "applicantId": {{{json request.params.id}}},
      "emailId": {{{json request.body.values.email}}},
      "candidateUserId": {{{json auth.artifacts.candidateUserId}}}
    }
  }
{{else}}
  null
{{/with}}
