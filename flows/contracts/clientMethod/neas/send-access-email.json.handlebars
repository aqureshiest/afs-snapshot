{{#if (and
  (contract 'actions' 1)
  (eq (lookup (contract 'actions' 1) "action") "identify")
  request.params.id
)}}
  {
    "action": "send-access-email",
    "client": "NeasClient",
    "method": "post",
    "uri": "auth/identity/access-code/send",
    "body": {
      "applicationId": {{{json application.id}}},
      "emailId": {{{json request.body.values.email}}}
    }
  }
{{else}}
  null
{{/if}}
