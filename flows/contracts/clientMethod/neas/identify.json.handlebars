{{#with (contract 'actions' (sum @index -1)) as |event|}}
  {
    "action": "identify",
    "client": "NeasClient",
    "method": "post",
    "uri": "/auth/identity/identify",
    "body": {
      "emailId": {{{json request.body.values.email}}},
      "idToken": {{{ json (coalesce request.headers.idtoken request.headers.idToken) }}}
    }
  }
{{else}}
  null
{{/with}}
