{{#if (and auth.artifacts.userId (eq request.body.values.type "page"))}}
  {
    "redisMethod": "setApplicationState",
    "key": "{{application.id}}_{{auth.artifacts.userId}}",
    "value": {
      {{#if request.body.flow.manifest}}
        "manifest": "{{{request.body.flow.manifest}}}",
      {{/if}}
        "step": {{{json request.body.flow.step}}}
    }
  }
  {{else}}
    {}
{{/if}}
