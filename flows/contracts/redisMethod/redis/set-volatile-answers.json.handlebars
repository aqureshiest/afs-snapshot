

{{#if (and auth.artifacts.userId (or request.body.values.employmentType request.body.values.incomeIsConsistent request.body.values.addAdditionalIncome request.body.values.sixMonthResidence))}}
  {
    "redisMethod": "setUserState",
    "key": "{{auth.artifacts.userId}}",
    "value": 
      {{#with request.body.values}}
        {{#obj}}
          {{#if employmentType}}
            {"employmentType": "{{employmentType}}"}
          {{/if}}
          {{#if incomeIsConsistent}}
            {"incomeIsConsistent": "{{incomeIsConsistent}}"}
          {{/if}}
          {{#if addAdditionalIncome}}
            {"addAdditionalIncome": "{{addAdditionalIncome}}"}
          {{/if}}
          {{#if sixMonthResidence}}
            {"sixMonthResidence": "{{sixMonthResidence}}"}
          {{/if}}
        {{/obj}}
      {{/with}}
  }
{{else}}
  {}
{{/if}}
