

{{#if (and auth.session.userId (or request.body.values.employmentType request.body.values.incomeIsConsistent request.body.values.addAddtionalIncome request.body.values.sixMonthResidence))}}
  {
    "redisMethod": "setUserState",
    "key": "{{auth.session.userId}}",
    "value": 
      {{#with request.body.values}}
        {{#obj}}
          {{#if employmentType}}
            {"employmentType": "{{employmentType}}"}
          {{/if}}
          {{#if incomeIsConsistent}}
            {"incomeIsConsistent": "{{incomeIsConsistent}}"}
          {{/if}}
          {{#if addAddtionalIncome}}
            {"addAddtionalIncome": "{{addAddtionalIncome}}"}
          {{/if}}
          {{#if sixMonthResidence}}
            {"sixMonthResidence": "{{sixMonthResidence}}"}
          {{/if}}
        {{/obj}}
      {{/with}}
  }
{{else}}
  {}
{{/if}}
