{{! cosigner creation requested }}
{{#if (not (includes application.tags "cosigned"))}}
  {
    "event": "create",
    "payload": {
      "application": {
        "primary": {
          "id": "{{ application.primary.id }}"
        },
        "root": {
          "id": "{{ application.id }}"
        },
        "details": {
          "name": {{{json request.body.values.name}}},
          "email": "{{request.body.values.email}}"
        }
      }
    },
    "fields": "application { id root { id } }"
  }
{{else}}
  null
{{/if}}
  
