{{! LA-1584: accountless applications are created with the applicant as the focal point;
    applications created in this way will have an NEAS session created with the applicationId
    corresponding to the first applicant created.
}}
{
  "event": "create",
  "rehydrate": false,
  "payload": {
    "application" : {
      "details": {
        "deviceId": "{{userState.deviceId}}"
      },
      "root": {
        "reference":{
          "brand":"earnest",
          "product":"slr"
        }
      }
    }
  },
  "fields": "application { id root { id }}"
}
